<div class="container">
  <h1 class="admin_agency_title"><%= @agency.name %></h1>

  <div class="left">
    <div class="center">
      <h5>Agency Details</h5>
      <%= link_to "Edit Details", edit_admin_agency_path(@agency) %>
    </div>
      <%= render "agency_details", agency: @agency %>
  </div>

  <div class="right">
    <h5>Admins</h5>
    <%= link_to "Add an Admin", new_admin_agency_user_path(@agency) %>
      <% @admins.each do |admin| %>
        <p><%= admin.username %>, <%= admin.email %></p>
      <% end %>
  </div>
</div>
