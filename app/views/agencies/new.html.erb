<%= form_for @agency do |f| %>
  <%= f.label :name %>
  <%= f.text_field :name %>

  <%= f.label :contact %>
  <%= f.text_field :contact %>

  <%= f.label :phone %>
  <%= f.telephone_field :phone %>

  <%= f.label :address %>
  <%= f.text_field :address %>

  <%= f.label :city %>
  <%= f.select :city, options_for_select(['Manhattan', 'Bronx', 'Queens', 'Brooklyn', 'Staten Island']) %>

  <%= f.label :zipcode %>
  <%= f.text_field :zipcode %>

  <%= f.label :notes %>
  <%= f.text_area :notes %>

  <%= f.label :food_type %>
  <%= f.select :food_type, options_for_select(['Cans', 'Prepared Food', 'Packaged Food', 'Frozen Food']) %>

  <%= f.submit %>
<% end %>


